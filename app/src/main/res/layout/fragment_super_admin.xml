<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".Admin.Model.SuperAdmin">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_3sdp"

        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:id="@+id/all_bookimg"
                android:background="@drawable/rect_dashboad"
                android:layout_gravity="center"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:padding="5dp">

                <ImageView
                    android:layout_width="52dp"
                    android:background="@drawable/circle_oval"
                    android:backgroundTint="@color/grey_bd"
                    android:layout_height="52dp"
                    android:padding="12dp"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_booking" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="All Booking"
                    android:textColor="@color/black"
                    android:textSize="13sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_3sdp"
                android:id="@+id/all_Add_Service_Boy"
                android:background="@drawable/rect_dashboad"
                android:layout_gravity="center"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:padding="5dp">

                <ImageView
                    android:layout_width="52dp"
                    android:background="@drawable/circle_oval"
                    android:backgroundTint="@color/grey_bd"
                    android:layout_height="52dp"
                    android:padding="12dp"
                    android:layout_gravity="center"
                    android:src="@drawable/servies_boyadd_user"
                    card_view:tint="@color/colorPrimary" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Add Service  Boy"
                    android:textColor="@color/black"
                    android:textSize="13sp" />


            </LinearLayout>






        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3sdp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:id="@+id/all_my_income"
                android:background="@drawable/rect_dashboad"
                android:layout_gravity="center"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:padding="5dp">

                <ImageView
                    android:layout_width="52dp"
                    android:background="@drawable/circle_oval"
                    android:backgroundTint="@color/grey_bd"
                    android:layout_height="52dp"
                    android:padding="12dp"
                    android:layout_gravity="center"
                    android:src="@drawable/money_ic"
                    card_view:tint="@color/colorPrimary" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="My incode"
                    android:textColor="@color/black"
                    android:textSize="14sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_5sdp"
                android:background="@drawable/rect_dashboad"
                android:layout_gravity="center"
                android:id="@+id/li_Service_Boy_List"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:padding="5dp">

                <ImageView
                    android:layout_width="52dp"
                    android:background="@drawable/circle_oval"
                    android:backgroundTint="@color/grey_bd"
                    android:layout_height="52dp"
                    android:padding="12dp"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_servies_boy"
                    card_view:tint="@color/colorPrimary" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Service Booking"
                    android:textColor="@color/black"
                    android:textSize="14sp" />


            </LinearLayout>






        </LinearLayout>





    </LinearLayout>



</FrameLayout>