<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".Admin.AdminDashboadFragment">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_3sdp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:id="@+id/all_bookimg"
            android:background="@drawable/rect_dashboad"
            android:layout_gravity="center"
            android:layout_height="@dimen/_90sdp"
            android:orientation="vertical"
            android:padding="5dp">

            <ImageView
                android:layout_width="@dimen/_50sdp"
                android:background="@drawable/circle_oval"
                android:backgroundTint="@color/grey_bd"
                android:layout_height="@dimen/_50sdp"
                android:padding="@dimen/_12sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_booking" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:gravity="center"
                android:text="All Booking"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_5sdp"
            android:background="@drawable/rect_dashboad"
            android:layout_gravity="center"
            android:id="@+id/li_Service_Boy_List"
            android:layout_height="@dimen/_90sdp"
            android:orientation="vertical"
            android:padding="5dp">

            <ImageView
                android:layout_width="@dimen/_50sdp"
                android:background="@drawable/circle_oval"
                android:backgroundTint="@color/grey_bd"
                android:layout_height="@dimen/_50sdp"
                android:padding="@dimen/_12sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_servies_boy"
                card_view:tint="@color/colorPrimary" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:gravity="center"
                android:text="Service Boy List"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp" />


        </LinearLayout>








    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_3sdp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:id="@+id/all_my_income"
            android:background="@drawable/rect_dashboad"
            android:layout_gravity="center"
            android:layout_height="@dimen/_90sdp"
            android:orientation="vertical"
            android:padding="5dp">

            <ImageView
                android:layout_width="@dimen/_50sdp"
                android:background="@drawable/circle_oval"
                android:backgroundTint="@color/grey_bd"
                android:layout_height="@dimen/_50sdp"
                android:padding="@dimen/_12sdp"
                android:layout_gravity="center"
                android:src="@drawable/money_ic"
                card_view:tint="@color/colorPrimary" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:gravity="center"
                android:text="Total Income"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp" />


        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_3sdp"
            android:id="@+id/all_Add_Service_Boy"
            android:background="@drawable/rect_dashboad"
            android:layout_gravity="center"
            android:layout_height="@dimen/_90sdp"
            android:orientation="vertical"
            android:padding="5dp">

            <ImageView
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_gravity="center"
                android:background="@drawable/circle_oval"
                android:backgroundTint="@color/grey_bd"
                android:padding="@dimen/_12sdp"
                android:src="@drawable/money_ic"
                card_view:tint="@color/colorPrimary" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:gravity="center"
                android:text="Today Income"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp" />
        </LinearLayout>

    </LinearLayout>

        <LinearLayout

            android:id="@+id/addModel_Number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_90sdp"
            android:layout_gravity="center"
            android:background="@drawable/rect_dashboad"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/_5sdp"
          >

            <ImageView
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_gravity="center"
                android:background="@drawable/circle_oval"
                android:backgroundTint="@color/grey_bd"
                android:padding="@dimen/_12sdp"
                android:src="@drawable/servies_boyadd_user"
                card_view:tint="@color/colorPrimary" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5ssp"
                android:gravity="center"
                android:text="Add Car Model "
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp" />


        </LinearLayout>

    </LinearLayout>


</FrameLayout>